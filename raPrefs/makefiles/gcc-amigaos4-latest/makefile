# makefile for GNU make (automatically generated by makemake)
# NOTE: indent with TAB in GNU makefiles!

# paths are relative to the project directory (current directory during make)

EXE    = bin/gcc-amigaos4-latest/OpenURLPrefs
OBJDIR = o/gcc-amigaos4-latest/

# target 'all' (default target)

all : $(EXE)

$(OBJDIR)OpenURL.o : OpenURL.c browsers.h gui_global.h
	gcc $(shell gccprefs) -c -o $(OBJDIR)OpenURL.o OpenURL.c

$(OBJDIR)loc.o : loc.c
	gcc $(shell gccprefs) -c -o $(OBJDIR)loc.o loc.c

$(OBJDIR)handlers.o : handlers.c handlers.h gui_global.h
	gcc $(shell gccprefs) -c -o $(OBJDIR)handlers.o handlers.c

$(OBJDIR)browsers.o : browsers.c browsers.h gui_global.h
	gcc $(shell gccprefs) -c -o $(OBJDIR)browsers.o browsers.c

$(OBJDIR)mailers.o : mailers.c mailers.h gui_global.h
	gcc $(shell gccprefs) -c -o $(OBJDIR)mailers.o mailers.c

$(OBJDIR)ftps.o : ftps.c ftps.h gui_global.h
	gcc $(shell gccprefs) -c -o $(OBJDIR)ftps.o ftps.c

$(OBJDIR)utility.o : utility.c utility.h
	gcc $(shell gccprefs) -c -o $(OBJDIR)utility.o utility.c

OBJS = $(OBJDIR)OpenURL.o $(OBJDIR)loc.o $(OBJDIR)handlers.o $(OBJDIR)browsers.o $(OBJDIR)mailers.o $(OBJDIR)ftps.o $(OBJDIR)utility.o

$(EXE) : $(OBJS)
	gcc $(OBJS) $(shell gccprefs) -o $(EXE)

# target 'strip'

strip:
	strip --remove-section=.comment $(EXE)

# target 'clean'

clean:
	-delete $(EXE)
	-delete $(OBJDIR)\*.o
